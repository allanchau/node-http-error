{
  "version": "2.1.1",
  "name": "allanchau-http-error",
  "description": "Simple Express HTTP error handler.",
  "keywords": [
    "api",
    "error",
    "errorhandler",
    "express",
    "handler",
    "http",
    "json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/allanchau/node-http-error"
  },
  "author": "Allan Chau <allan.chau@icloud.com> (http://allanchau.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allanchau/node-http-error/issues"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/eslint */*.js */**/*.js --quiet --ignore-pattern node_modules --ignore-pattern coverage",
    "report-coverage": "codecov",
    "test": "./node_modules/.bin/npm-run-all --parallel lint test-lib",
    "test-lib": "jest"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "codecov": "^3.5.0",
    "eslint": "^6.0.1",
    "eslint-config-allanchau": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-jest": "^22.7.1",
    "express": "^4.17.1",
    "husky": "^2.7.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "prettier-eslint-cli": "^5.0.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "pino": "^5.12.6"
  }
}
